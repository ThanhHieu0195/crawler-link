# /bin/bash

# ubuntu1
sudo chown -R hieutct:hieutct /home/hieutct/syn/crawler-link/
sudo rm -rf /home/hieutct/syn/crawler-link/.env
port=$(date +"%I%S")
echo "TYPE=fb
IP_ADDRESS=localhost
PORT=$port
NUM_CLIENT=10
CLIENT_TYPE=ins
MONGO_HOST=localhost
MONGO_DATABASE=crawler" >> /home/hieutct/syn/crawler-link/.env

